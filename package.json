{
    "name": "@apple/hierarchical-confusion-matrix",
    "author": "Apple Inc.",
    "version": "1.1.1",
    "license": "SEE LICENSE IN `LICENSE`",
    "files": [
        "src",
        "package",
        "README.md"
    ],
    "main": "./package/confMat.umd.cjs",
    "module": "./package/confMat.js",
    "exports": {
        ".": {
            "import": "./package/confMat.js",
            "require": "./package/confMat.umd.cjs"
        },
        "./package/*.css": {
            "import": "./package/*.css",
            "require": "./package/*.css"
        }
    },
    "types": "./dist/index.d.ts",
    "style": "./dist/style.css",
    "type": "module",
    "devDependencies": {
        "@sveltejs/adapter-static": "^2.0.2",
        "@sveltejs/kit": "^1.24.0",
        "@sveltejs/vite-plugin-svelte": "^2.4.5",
        "@types/cwise": "^1.0.4",
        "@types/d3-array": "^3.0.5",
        "@types/d3-dsv": "^3.0.2",
        "@types/d3-scale": "^4.0.3",
        "@types/lodash": "^4.14.195",
        "@types/ndarray": "^1.0.11",
        "@types/node": "^20.5.7",
        "@typescript-eslint/eslint-plugin": "^5.59.6",
        "@typescript-eslint/parser": "^5.62.0",
        "eslint": "^8.50.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-json": "^3.1.0",
        "eslint-plugin-svelte": "^2.33.2",
        "prettier": "^2.8.8",
        "prettier-plugin-svelte": "^2.10.1",
        "svelte": "^3.59.2",
        "svelte-check": "^3.5.2",
        "tslib": "^2.6.2",
        "typescript": "^5.1.6",
        "vite": "^4.3.9",
        "vite-plugin-dts": "^3.5.1",
        "vitest": "^0.34.1"
    },
    "scripts": {
        "build": "vite build",
        "build:lib": "vite build --config vite.config.lib.ts",
        "prepack": "yarn build:lib",
        "dev": "vite dev",
        "preview": "vite preview",
        "coverage": "vitest run --coverage",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
        "test:unit": "vitest",
        "fmt": "prettier --ignore-path .eslintignore --check .",
        "lint": "eslint . --max-warnings 0",
        "fix:lint": "eslint . --fix",
        "fix:fmt": "prettier --ignore-path .eslintignore --write ."
    },
    "dependencies": {
        "d3-array": "^3.2.4",
        "d3-dsv": "^3.0.1",
        "d3-scale": "^4.0.2",
        "lodash": "^4.17.21",
        "ndarray": "^1.0.19",
        "ndarray-ops": "^1.2.2",
        "vega-scale": "^7.3.0"
    }
}
